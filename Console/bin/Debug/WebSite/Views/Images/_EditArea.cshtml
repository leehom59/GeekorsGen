@model JamZooMng.Models.ImagesModel
    <div class="editArea">
        <h2>
			<span class="notes">標有「<img src="@Url.Content("~/images/icon/bullet-red-star.gif")">」為必填項目：</span></h2>
             @Html.ValidationSummary(true, "新增失敗")
        <table border="0" cellpadding="0" cellspacing="0" class="tableStyle01">
            <tbody>
                
				<tr>
					<td width="90" class="star1">
						Fid
					</td>
					<td>
						@Html.TextBoxFor(p => p.Fid, new { @class = "txt1", size = "30"})
						@Html.ValidationMessageFor(p => p.Fid)
					</td>
				</tr>

				<tr>
					<td width="90" class="star1">
						Cname
					</td>
					<td>
						@Html.TextBoxFor(p => p.Cname, new { @class = "txt1", size = "30"})
						@Html.ValidationMessageFor(p => p.Cname)
					</td>
				</tr>

				<tr>
					<td width="90" class="star1">
						Orders
					</td>
					<td>
						@Html.TextBoxFor(p => p.Orders, new { @class = "txt1", size = "30"})
						@Html.ValidationMessageFor(p => p.Orders)
					</td>
				</tr>

				<tr>
					<td width="90" class="star1">
						Create_Date
					</td>
					<td>
						@Html.Label(Model.Create_Date.ToString("yyyy-MM-dd"))
					</td>
				</tr>


            </tbody>
        </table>
    </div>

	@{
		string _Controller = Request.RequestContext.RouteData.Values["controller"].ToString();
	}

    <div class="BTNcenter">
        <input type="submit" class="btn" id="btn_submit" name="btn_submit" value="儲存送出" onclick="javascript:return confirm('確定儲存送出嗎?');">
        <input type="button" class="btn" id="btn_cancle" name="btn_cancle" value="取消變更" onclick="javascript:if (confirm('確定取消變更嗎?')){window.location.href='@Url.Content("~/" + _Controller + "/List")';}">
    </div>